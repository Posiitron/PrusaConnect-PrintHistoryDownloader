{
  "manifest_version": 3,
  "name": "Prusa Print History Downloader",
  "version": "2.1.0",
  "description": "Extract print history from Prusa Connect and save it as a CSV file.",
  "permissions": [
    "activeTab",
    "downloads"
  ],
  "host_permissions": [
    "https://connect.prusa3d.com/*"
  ],
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
  "web_accessible_resources": [
    {
      "resources": ["scripts/jszip.js"],
      "matches": ["<all_urls>"]
    }
  ]
}

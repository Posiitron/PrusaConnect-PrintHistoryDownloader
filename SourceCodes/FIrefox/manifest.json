{
  "manifest_version": 3,
  "name": "Prusa Print History Downloader",
  "version": "2.1.0",
  "description": "Extract print history from Prusa Connect and save it as a CSV file.",
  "permissions": [
    "activeTab",
    "downloads",
    "storage"
  ],
  "host_permissions": [
    "https://connect.prusa3d.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "32": "icons/icon32.png"
    }
  },
  "icons": {
    "48": "icons/icon48.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "csvdataextractor@example.com",
      "strict_min_version": "109.0"
    }
  }
}
